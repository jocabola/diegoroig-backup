<script type="text/javascript">

var gallery = [];
var obj;

	obj = {};

	obj.thumb = '../../content/work/5.les-variacions-goku/1.goku/thumb.jpg';
	obj.title = 'Goku';
	obj.description = '2010. oil on board. 200 x 200 cm.';

	obj.image = {};

			obj.image.url ='../../content/work/5.les-variacions-goku/1.goku/LES-VARIACIONS-GOKU.jpg';
		obj.image.ratio = 1400 / 1400;
	

	gallery.push( obj );



	var nthumbs = 7;


new Diego.Project( gallery, nthumbs );

</script>

